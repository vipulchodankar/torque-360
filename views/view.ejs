<div class="container">
  <table class="table table-striped table-hover table-responsive my-5 pb-5">
    <thead>
      <tr>
        <th>No.</th>
        <th>Model</th>
        <th>Variant</th>
        <th>Company</th>
        <th>Mileage</th>
        <th>Cylinders</th>
        <th>Clearance</th>
        <th>Horsepower</th>
        <th>Topspeed</th>
        <th>Airbags</th>
        <th>FuelType</th>
        <th>Image</th>
        <th>Price</th>
        <th>Added By</th>
      </tr>
    </thead>

    <tbody>
      <% cars.forEach(function (car, index) { %>
      <tr>
        <td><%= index+1 %></td>
        <td><%= car.model %></td>
        <td><%= car.variant %></td>
        <td><%= car.company %></td>
        <td><%= car.mileage %> KM/L</td>
        <td><%= car.cylinders %></td>
        <td><%= car.clearance %>cm</td>
        <td><%= car.horsepower %> BHP</td>
        <td><%= car.topspeed %> KM/H</td>
        <td><%= car.airbags %></td>
        <td><%= car.fueltype %></td>
        <td>
          <a href="<%= car.image %>"
            ><img
              src="<%= car.image %>"
              class="img-fluid"
              alt="<%= car.model %>"
            />
          </a>
        </td>
        <td>â‚¹<%= car.price %>L</td>
        <td>
          <a href="mailto:<%= car.editor %>" target="_blank"
            ><%= car.editor %></a
          >
        </td>
      </tr>
      <% }) %>
    </tbody>
  </table>

  <a href="/dashboard" class="btn btn-primary my-2">Go Back</a>
</div>
